opcode_mode = single;
import tset TSet4;

instruments = {
     vdda : DCVS 1;
}
srm_vector ($tset, AllDig:S)
{
     // ___ THIS PATTERN STARTS WITH vdda VOLTAGE at 0.0V  ___
     > TSet4  00000000000000000000000000000000000 ;
     > TSet4  00000000000000000000000000000000000 ;
     repeat 5000
     > TSet4  00000000000000000000000000000000000 ;
     (vdda : DCVS = PSet my_pset_1)
     repeat 5000
     > TSet4  00000000000000000000000000000000000 ;
     (vdda : DCVS = strobe)
     > TSet4  00000000000000000000000000000000000 ;
     > TSet4  00000000000000000000000000000000000 ;
     // ___ VBUMP to 3.3 V ___
     (vdda : DCVS = VS_Alt_Sel)
     > TSet4  00000000000000000000000000000000000 ;
     repeat 5000
     > TSet4  00000000000000000000000000000000000 ;
     > TSet4  00000000000000000000000000000000000 ;
     (vdda : DCVS = strobe)
     > TSet4  00000000000000000000000000000000000 ;
     > TSet4  00000000000000000000000000000000000 ;
     > TSet4  00000000000000000000000000000000000 ;
     > TSet4  00000000000000000000000000000000000 ;
     > TSet4  00000000000000000000000000000000000 ;
}

